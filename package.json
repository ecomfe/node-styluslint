{
  "name": "styluslint",
  "description": "lint your stylus code",
  "version": "0.0.1-beta.3",
  "keywords": [
    "styluslint",
    "stylushint",
    "lint",
    "hint"
  ],
  "maintainers": [
    {
      "name": "ielgnaw",
      "email": "wuji0223@gmail.com"
    }
  ],
  "dependencies": {
    "chalk": "^1.1.0",
    "stylus": "^0.51.1"
  },
  "devDependencies": {
    "circular-json": "^0.1.6",
    "jison": "^0.4.15",
    "jison-lex": "^0.3.4",
    "json-circular": "0.0.1",
    "json-stringify-safe": "^5.0.1",
    "lex": "^1.7.8"
  },
  "scripts": {
    "lint": "fecs lib cli test/**/*.spec.js --type=js",
    "coverage": "istanbul cover jasmine-node --captureExceptions test/**/*.spec.js",
    "test": "npm run lint && npm run coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "bin": {},
  "repository": {
    "type": "git",
    "url": "git@github.com:ecomfe/node-styluslint.git"
  }
